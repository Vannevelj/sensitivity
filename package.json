{
  "name": "sensitivity",
  "version": "1.9",
  "private": true,
  "description": "A Github Action that detects sensitive information in your code",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "npm build && ncc build --source-map --license licenses.txt && git add . && git commit -m \"Generated release files\"",
    "test": "jest",
    "all": "npm run build && npm run format && npm test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vannevelj/sensitivity.git"
  },
  "keywords": [
    "actions",
    "emails",
    "secrets",
    "sensitive"
  ],
  "author": "Jeroen Vannevel",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@actions/glob": "^0.2.0",
    "@octokit/rest": "^18.9.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^15.14.1",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.2.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.1.3"
  }
}
